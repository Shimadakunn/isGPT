{
    "name": "isGPT",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Extension that indicates you if it is generated by ChatGPT",
    "action":{
      "default_popup": "index.html"
    },
    "permissions": ["scripting", "activeTab","storage"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]    
}